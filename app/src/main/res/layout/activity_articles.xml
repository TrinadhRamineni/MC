<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin.devika.ArticlesActivity">

    <TextView
        android:id="@+id/taArticles"
        android:layout_width="345dp"
        android:layout_height="394dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="21dp"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="24dp" />


    <Button
        android:id="@+id/btnNext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btnPrevious"
        android:layout_alignBottom="@+id/btnPrevious"
        android:layout_alignEnd="@+id/taArticles"
        android:layout_alignRight="@+id/taArticles"
        android:text="Next"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="447dp" />

    <Button
        android:id="@+id/btnPrevious"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/taArticles"
        android:layout_alignStart="@+id/taArticles"
        android:layout_below="@+id/taArticles"
        android:layout_marginTop="19dp"
        android:text="Previous"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="446dp" />

</RelativeLayout>
